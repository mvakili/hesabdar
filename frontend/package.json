    "back-publish": "dotnet publish -c release -r win10-x64 ../backend/Hesabdar",
    "back-copy": "xcopy /E \"../backend/Hesabdar/bin/release/netcoreapp2.1\" \"./Hesabdar-win32-x64\" /y",
    "copy-index": "copy \"./electronindex.js\" \"./dist/index.js\" && copy \"./electronPackage.json\" \"./dist/package.json\"",
